<head>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</head>
<div id="loadtimeChart" style="width: 100%; height: 300px;display: inline-block;"></div>
<div id="loadtimeCitiesChart" style="width: 100%; height: 300px;display: inline-block;"></div>
<script type="text/javascript">
        var chart1 = new CanvasJS.Chart("loadtimeChart", {
            theme: "theme2",
            animationEnabled: true,
            title: {
                text: "Loadtime of Different Websites"
            },
            axisX: {
                interval: 1
            },
            axisY: {
                title: "Loadtime (seconds)"
            },
            data: [
                {
                    type: "column", //change type to bar, line, area, pie, etc
                    dataPoints: @Html.Raw(ViewBag.DataPoints),
                }
            ]
    });
    var chart2 = new CanvasJS.Chart("loadtimeCitiesChart", {
            theme: "theme2",
            animationEnabled: true,
            title: {
                text: "Avg. Loadtime of Different Locations"
            },
            axisX: {
                interval: 1
            },
            axisY: {
                title: "Loadtime (seconds)"
            },
            data: [
                {
                    type: "column", //change type to bar, line, area, pie, etc
                    dataPoints: @Html.Raw(ViewBag.DataPoints1),
                }
            ]
        });
    chart1.render();
    chart2.render();
</script>

