@using PingItWebsite.Models;

<table>
    <tr>
        <td>TimeStamp</td>
        <td>URL</td>
        <td>WebSpeed(Mbps)</td>
        <td>LoadTime(s)</td>
        <td>PageSize(kb)</td>
        <td>Location</td>
        <td>Platform</td>
    </tr>
    @foreach(WebTest test in Model)
    {
        <tr>
            <td>@test.date.ToString()</td>
            <td>@test.url</td>
            <td>@test.webspeed.ToString()</td>
            <td>@test.loadtime.ToString()</td>
            <td>@test.pagesize.ToString()</td>
            <td>@test.location</td>
            <td>@test.browser</td>
        </tr>

    }
</table>
